{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "backend",
      "image": "193264398822.dkr.ecr.eu-west-1.amazonaws.com/pyconquiz:latest",
      "essential": true,
      "memoryReservation": 850,
      "portMappings": [
        {
          "hostPort": "80",
          "containerPort": "8000"
        }
      ]
    },
    {
      "name": "worker",
      "image": "193264398822.dkr.ecr.eu-west-1.amazonaws.com/pyconquiz:latest",
      "essential": true,
      "memoryReservation": 50,
      "command": ["python", "manage.py", "rqworker"]
    },
    {
      "name": "scheduler",
      "image": "193264398822.dkr.ecr.eu-west-1.amazonaws.com/pyconquiz:latest",
      "essential": true,
      "memoryReservation": 50,
      "command": ["python", "manage.py", "rqscheduler", "-i", "1"]
    }
  ]
}
